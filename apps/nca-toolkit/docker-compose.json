{
  "services": [
    {
      "name": "nca-toolkit",
      "image": "stephengpope/no-code-architects-toolkit:latest",
      "isMain": true,
      "internalPort": "8080",
      "environment": {
        "API_KEY": "${API_KEY}",
        "S3_ENDPOINT_URL": "${S3_ENDPOINT_URL}",
        "S3_ACCESS_KEY": "${S3_ACCESS_KEY}",
        "S3_SECRET_KEY": "${S3_SECRET_KEY}",
        "S3_BUCKET_NAME": "${S3_BUCKET_NAME}",
        "S3_REGION": "${S3_REGION}"
      }
    }
  ]
}